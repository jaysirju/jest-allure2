{
  "name": "jest-allure2",
  "version": "1.1.0",
  "description": "Allure Reports for jest",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "author": "Ryan Parker",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --config ./__test__/jest.config.js",
    "lint": "xo ./src && prettier ./src --list-different",
    "fix": "xo ./src --fix && prettier ./src --write",
    "commit": "git-cz"
  },
  "keywords": [
    "jest",
    "report",
    "reports",
    "allure",
    "tests"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/allure-js-commons": "^0.0.1",
    "@types/jasmine": "^3.5.10",
    "@types/jest": "^25.2.1",
    "commitizen": "^4.0.5",
    "conventional-changelog-conventionalcommits": "^4.2.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "typescript": "^3.8.3",
    "xo": "^0.30.0"
  },
  "dependencies": {
    "allure-jasmine": "^2.0.0-beta.6",
    "allure-js-commons": "^2.0.0-beta.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryparker/jest-allure.git"
  },
  "xo": {
    "globals": [
      "jasmine",
      "allure"
    ],
    "semicolon": false,
    "code": 120,
    "prettier": true,
    "rules": {
      "@typescript-eslint/prefer-readonly-parameter-types": 0
    }
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "config": "./node_modules/cz-conventional-changelog"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# Changelog"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
